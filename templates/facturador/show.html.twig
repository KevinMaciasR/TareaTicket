            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_facturador_index') }}">back to list</a>

    <a href="{{ path('app_facturador_edit', {'idFacturador': facturador.idFacturador}) }}">edit</a>

    {{ include('facturador/_delete_form.html.twig') }}
