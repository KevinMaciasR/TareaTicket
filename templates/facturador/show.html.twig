    <table class="table">
        <tbody>
            <tr>
                <th>IdFacturador</th>
                <td>{{ facturador.idFacturador }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ facturador.nombre }}</td>
            </tr>
            <tr>
                <th>Cedula</th>
                <td>{{ facturador.cedula }}</td>
            </tr>
            <tr>
                <th>Usuario</th>
